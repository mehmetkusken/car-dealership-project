<h1>Edit the Infortmation</h1>
<div id="list">
<form action="/carlist/<%= @car.id %>" method="post">
<input type="hidden" name="_method" value= "patch">
  <label for="make">Make:</label>
  <input type="text" id="make" name="car[make]"><br>
  <br>
  <label for="model">Model:</label>
  <input type="text" id="model" name="car[model]"><br>
  <br>
  <label for="year">Year:</label>
  <input type="number" id="year" name="car[year]"><br>
  <br>
  <label for="color">Color:</label>
  <input type="text" id="color" name="car[color]"><br>
  <br>
  <label for="price">Price:</label>
  <input type="number" id="price" name="car[price]"><br>
  <br>
  <label for="urlpictures">Pictures:</label>
  <input type="url" id="urlpictures" name="car[urlpictures]">
  <br>
  <br>
  <input type="submit" value="Submit Form"><br>
</form>
</div>